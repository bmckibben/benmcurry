<div id="<%= dom_id wiki %>">
  <p>
    <strong>Title:</strong>
    <%= wiki.title %>
  </p>

  <p>
    <strong>User:</strong>
    <%= wiki.user_id %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= wiki.body %>
  </p>

  <p>
    <strong>Parent:</strong>
    <%= wiki.parent %>
  </p>

  <p>
    <strong>Version:</strong>
    <%= wiki.version %>
  </p>

  <p>
    <strong>Integer:</strong>
    <%= wiki.integer %>
  </p>

  <p>
    <strong>Deleted:</strong>
    <%= wiki.deleted %>
  </p>

  <p>
    <strong>Default sort:</strong>
    <%= wiki.default_sort %>
  </p>

  <p>
    <strong>Last revision:</strong>
    <%= wiki.last_revision %>
  </p>

</div>
